      <h2><%= pluralize(@w_number.errors.count, "error") %> prohibited this w_number from being saved:</h2>

      <ul>
      <% @w_number.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :number %><br>
    <%= f.text_field :number %>
  </div>
  <div class="field">
    <%= f.label :darwing_number %><br>
    <%= f.text_field :darwing_number %>
  </div>
  <div class="field">
    <%= f.label :year %><br>
    <%= f.text_field :year %>
  </div>
  <div class="field">
    <%= f.label :standard %><br>
    <%= f.text_field :standard %>
  </div>
  <div class="field">
    <%= f.label :tag_number %><br>
    <%= f.text_field :tag_number %>
  </div>
